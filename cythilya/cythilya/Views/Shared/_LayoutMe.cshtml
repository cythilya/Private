<link rel="stylesheet" href="/Content/me/css/lessframework.css" media="all">
<link rel="stylesheet" href="/Content/me/css/flexslider.css">
</head>
<body class="home">
<header itemscope itemtype="http://schema.org/WPHeader">
	<div class="wrapper cf">
        @Html.Partial("_HeaderMe")
        @{
            if (enableSlider == true)
            {
                @Html.Partial("_SliderMe");
            }    
        }
	</div>
</header>
<div id="main">
	<div class="wrapper cf">
        @RenderBody()
	</div>
</div>
@Html.Partial("_FooterMe")
